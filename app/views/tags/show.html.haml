.jumbotron
  .container.text-center
    = image_tag("header_logo.png", alt: "PROTO TYPE - SHARE THE PROTOTYPE", height: "73px")

.container.proto-list
  .row
    .col-sm-4.col-md-3.proto-content
  .thumbnail
    - @tag.prototypes.each do |prototype|
      = link_to image_tag(prototype.set_main_thumbnail), prototype_path(prototype)
      .caption
        %h3
          = prototype.title
        .proto-meta
          .proto-user
            = link_to prototype.user.name, user_path(prototype.user)
          .proto-posted
            = prototype.posted_date


.text-center
  %ul.pagination
    %li.disabled
      %a{"aria-label": "Previous", href: "#"}
        %span{"aria-hidden": "true"} «
    %li.active
      %a{href: "#"}
        1
        %span.sr-only (current)
    %li
      %a{href: "#"} 2
    %li
      %a{href: "#"} 3
    %li
      %a{href: "#"} 4
    %li
      %a{href: "#"} 5
